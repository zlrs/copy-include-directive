{
	"name": "copy--include",
	"displayName": "Copy C/C++ #include",
	"description": "Copy #include directive of the header file. ",
	"version": "0.0.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/zlrs/copy-include-directive.git"
	},
	"publisher": "KarlZhu",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:copy--include.currentFile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "copy--include.currentFile",
				"title": "Copy #include: Current opened file"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/copy-paste": "^1.1.30",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"copy-paste": "^1.3.0"
	}
}
